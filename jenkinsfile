pipeline{

    agent any

    stages{

        stage('Continuos Download') {

            steps{
                git branch: 'main', url: 'https://github.com/njokadeclan/DEVOPS-PROJECT2.git'
            }
        }
        stage('Unit Test'){

            steps{
                sh'mvn test'
            }
        }        
            }
        }
    }
}